{
  "manifest_version": 3,
  "name": "FirstFire - Window Merger",
  "version": "0.1.1",
  "description": "Merge all Chrome windows, group tabs together, and remove duplicates.",
  "permissions": [
    "tabs",
    "tabGroups",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
